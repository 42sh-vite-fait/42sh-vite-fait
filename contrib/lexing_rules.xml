<?xml version="1.0" encoding="UTF-8"?>

<grammar>

  <char_types>
	<none />
	<blank />
	<letter />
	<squote />
	<dquote />
	<pipe />
	<semi />
	<less />
	<great />
	<and />
	<newline />

	<lparen />
	<rparen />
	<dash />
	<bslash />
	<hash />
  </char_types>

  <state_types>
	<error />
	<start />
	<blank />
	<word />
	<pipe />
	<semi />
	<less />
	<dless />
	<lessand />
	<great />
	<dgreat />
	<greatand />
	<newline />
	<lessgreat />
	<dlessdash />
	<clobber />
	<and />
	<and_if />
	<or_if />
	<lparen />
	<rparen />
	<bslash />
	<comment />

  </state_types>

  <stack_types>
	<empty />
	<squote />
	<dquote />
  </stack_types>

  <table>
	<start>
	  <empty>
		<blank>blank</blank>
		<letter>word</letter>
		<squote push="squote">word</squote>
		<dquote push="dquote">word</dquote>
		<pipe>pipe</pipe>
		<semi>semi</semi>
		<less>less</less>
		<great>great</great>
		<and>and</and>
		<newline>newline</newline>
		<lparen>lparen</lparen>
		<rparen>rparen</rparen>
		<dash>word</dash>
		<bslash>bslash</bslash>
		<hash>comment</hash>
	  </empty>
	</start>

	<blank>
	  <empty>
		<blank>blank</blank>
	  </empty>
	</blank>

	<word>
	  <empty>
		<letter>word</letter>
		<squote push="squote">word</squote>
		<dquote push="dquote">word</dquote>
		<dash>word</dash>
		<bslash>bslash</bslash>
		<hash>word</hash>
	  </empty>
	  <squote fallback="word">
		<none>error</none>
		<squote pop="">word</squote>
	  </squote>
	  <dquote fallback="word">
		<none>error</none>
		<dquote pop="">word</dquote>
		<dollar>dollar</dollar>
		<bslash>bslash</bslash>
		<bquote push="bquote">word</bquote>
	  </dquote>
	</word>

	<less>
	  <empty fallback="error">
		<less>dless</less>
		<and>lessand</and>
		<great>lessgreat</great>
	  </empty>
	</less>

	<dless>
	  <empty>
		<dash>dlessdash</dash>
	  </empty>
	</dless>

	<great>
	  <empty>
		<great>dgreat</great>
		<and>greatand</and>
		<pipe>clobber</pipe>
	  </empty>
	</great>

	<and>
	  <empty>
		<and>and_if</and>
	  </empty>
	</and>

	<pipe>
	  <empty>
		<pipe>or_if</pipe>
	  </empty>
	</pipe>

	<newline>
	</newline>

	<bslash>
	  <empty fallback="word">
		<none>error</none>
	  </empty>
	  <dquote fallback="word">
		<none>error</none>
	  </dquote>
	</bslash>

	<comment fallback="comment">
	  <empty fallback="comment">
		<none>error</none>
		<newline>error</newline>
	  </empty>
	</comment>

  </table>

</grammar>
