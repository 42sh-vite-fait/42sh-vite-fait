AST:
LIST: ;
	LIST: ;
		SIMPLE_COMMAND: echo
			args#1: 'useless commands'
		SIMPLE_COMMAND: first=$(ls -1 2>/dev/null | head -n 1)
	AND_OR: ||
		AND_OR: &&
			SIMPLE_COMMAND: test
				args#1: -d
				args#2: ${first}
			SUBSHELL: ()
				redirections#0: IO=2, Operator=GREATAND, Word=1
				redirections#1: IO=1, Operator=GREAT, Word=output
				AND_OR: &&
					SIMPLE_COMMAND: touch
						args#1: ${first}/file
					PIPE_SEQUENCE: |
						PIPE_SEQUENCE: |
							SIMPLE_COMMAND: cat
								args#1: /dev/random
							SIMPLE_COMMAND: base64
						SIMPLE_COMMAND: head
							args#1: -c
							args#2: 42
		SIMPLE_COMMAND: mkdir
			args#1: ${first}
